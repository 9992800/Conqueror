package com.jctx.chat.module;

public class OnlineBean {
	
	public static final String SYSTEM_CHANNEL = "SYS_40010086";
	
	public static final int DEVICE_TYPE_IOS     = 1;
	
	public static final int DEVICE_TYPE_ANDROID = 2;
	
	int  device;
	String self, userName, token, sessionId;
	long system; 
	
	public String getSelf() {
		return self;
	}
	public void setSelf(String self) {
		this.self = self;
	}
	public int getDevice() {
		return device;
	}
	public void setDevice(int device) {
		this.device = device;
	}
	public String getUserName() {
		return userName;
	}
	public void setUserName(String userName) {
		this.userName = userName;
	}
	public String getToken() {
		return token;
	}
	public void setToken(String token) {
		this.token = token;
	}
	public long getSystem() {
		return system;
	}
	public void setSystem(long system) {
		this.system = system;
	}
	
	@Override
	public String toString() {
		return "OnlineBean [self=" + self + ", device=" + device + ", userName=" + userName + ", token=" + token
				+ ", sessionId=" + sessionId + ", system=" + system + "]";
	}
	
	public void setSessionId(String id) { 
		this.sessionId = id;
	} 
	public String getSessionId(){
		return this.sessionId;
	}
}
